<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:orientation="vertical"
                                             android:layout_height="match_parent"
                                             android:layout_width="match_parent" xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView
            android:layout_width="383dp"
            android:layout_height="61dp"
            android:textSize="20sp"
            app:fontFamily="@font/rubik_medium"
            android:id="@+id/total_weekly"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="140dp"/>

    <TableLayout
            android:id="@+id/friends_table"
            android:layout_width="386dp"
            android:layout_height="452dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/total_weekly"
            app:layout_constraintVertical_bias="0.0">


    </TableLayout>
    <TextView
            android:text="@string/new_friend_message"
            android:layout_width="343dp"
            android:layout_height="44dp"
            android:id="@+id/add_friend_message"
            app:fontFamily="@font/rubik_medium"
            android:layout_marginTop="107dp"
            app:layout_constraintTop_toBottomOf="@+id/friends_table" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="707dp" app:layout_constraintVertical_bias="0.511"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/friends_table"
            android:layout_marginStart="378dp"/>
    <Button
            android:text="@string/new_friend"
            android:id="@+id/add_friend_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="134dp"
            app:fontFamily="@font/rubik_medium"
            app:layout_constraintStart_toEndOf="@+id/friends_table" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="508dp" android:layout_marginTop="6dp"
            app:layout_constraintTop_toBottomOf="@+id/add_friend_message"/>



    <android.support.v4.widget.DrawerLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/container"
    />



    <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/frameLayout" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0">

    </FrameLayout>
</android.support.constraint.ConstraintLayout>